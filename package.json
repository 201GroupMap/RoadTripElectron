{
  "name": "roadtrip",
  "productName": "Road Trip Planner",
  "version": "1.0.1",
  "description": "CSCI 201 Group Project - Road Trip Planning App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Road-Trip-Planner-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=img/icon4.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Road Trip Planner\"",
    "package-linux": "electron-packager . road-trip-planner --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "\u0016Jamin Chen, Austin Bahng, Daniel Ho, Zexia Zhang",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "electron": "^1.7.9",
    "electron-oauth2": "^3.0.0",
    "electron-store": "^1.3.0",
    "jquery": "^3.2.1",
    "sortable.js": "^0.3.0",
    "sortablejs": "^1.7.0"
  },
  "devDependencies": {
    "electron-packager": "^9.0.1"
  }
}
